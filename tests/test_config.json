{
  "mcpServers": {
    "filesystem": {
      "command": "python",
      "type": "local",
      "port": 8083,
      "enabled": true,
      "auto_restart": false,
      "workdir": ".",
      "args": ["-m", "modelcontextprotocol.servers.filesystem"]
    },
    "browser": {
      "command": "playwright",
      "type": "npm",
      "port": 8084,
      "enabled": true,
      "auto_restart": false,
      "workdir": ".",
      "args": ["--target", "browser"]
    },
    "remote-test": {
      "url": "http://localhost:8085",
      "type": "remote",
      "enabled": false,
      "auto_restart": false
    }
  }
}
